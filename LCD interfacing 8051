ORG 00H
MOV A, #33H
ACALL CMN WRT
ACALL DELAY
MOV A, #0EH
ACALL CMN WRT
ACALL DELAY
MOV A, #06H
ACALL CMN WRT
ACALL DELAY
MOV A, #'C'
ACALL DAT WRT
ACALL DELAY
MOV A, #'M'
ACALL DAT WRT
ACALL DELAY
MOV A, #'R'
ACALL DAT WRT
ACALL DELAY
MOV A, #'I'
ACALL DAT WRT
ACALL DELAY
MOV A, #'T'
ACALL DAT WRT
ACALL DELAY
CMN WRTMOV P1, A
CLR P3.5
CLR P3.4
SETB P3.3
CLR P3.3
RET
DATWRT MOV P1, A
SETB P3.5
CLR P3.4
CLR P3.3
RET
DELAY MOV R3, #0FFH
MOV R4, #0FFH
MOV R5, #50H
HERE DJNZ R5, HERE
HERE1 DJNZ R4, HERE1
HERE2 DJNZ R3, HERE2
RET
END
