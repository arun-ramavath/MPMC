assume cs:code, ds:data, es:extra

data segment
    str1 db 'cmritlab'
data ends

extra segment
    str2 db 'javalab'
extra ends

code segment
start: 
    mov ax, data
    mov ds, ax
    mov ax, extra
    mov es, ax
    mov si, offset str1
    mov di, offset str2
    mov cl, 08h
    cld
L2: 
    cmpsb
    jne L1
    loop L2
    mov bx, 1111h
    jmp L3
L1: 
    mov bx, 0000h
L3: 
    int 03h
code ends
end start
