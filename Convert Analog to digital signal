MOV AL, 90H
MOV DX, 0FFC6H
OUT DX, AL

MOV AL, 07H
MOV DX, 0FFL4H
OUT DX, AL

MOV AL, 0FH
MOV DX, 0FFC6H
OUT DX, AL

MOV CX, 3FFFH
LOOP D1

MOV AL, 0EH
MOV DX, 0FFC6H
OUT DX, AL

MOV DX, 0FFC0H
IN DX

AND AL, 80H
CMP AL, 80H
JNZ DZ

MOV AL, 00H
MOV DX, 0FFC6H
OUT DX, AL

MOV DX, 0FFC0H
IN DX

MOV DX, 0FFC2H
OUT DX, AL

JMP D3
