ASSUME CS:CODE, DS:DATA

DATA SEGMENT
ARRAY DB 03H,09H,02H,06H,05H
RES DB 01H DUP(0)
DATA ENDS

CODE SEGMENT
START:
    MOV AX,DATA
    MOV DS,AX
    MOV SI,OFFSET ARRAY
    MOV CL,05H
    MOV AL,09H
UP: CMP AL,[SI]
    JE DOWN
    INC SI
    DEC CL
    JNZ UP
    MOV RES,'N'
    JMP L1
DOWN: MOV RES,'Y'
L1: INT 03H
CODE ENDS
END START
