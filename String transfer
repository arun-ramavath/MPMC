ASSUME CS:CODE, DS:DATA, ES:EXTRA

DATA SEGMENT
STR1 DB "CMRIT"
DATA ENDS

EXTRA SEGMENT
STR2 DB 05H DUP(0)
EXTRA ENDS

CODE SEGMENT
START:
    MOV AX,DATA
    MOV DS,AX
    MOV AX,EXTRA
    MOV ES,AX
    MOV SI,OFFSET STR1
    MOV DI,OFFSET STR2
    MOV CL,05H
    CLD
    REP MOVSB
    INT 03H
CODE ENDS
END START
